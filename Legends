local Toggle6 = Tab:Toggle({
    Title = "自动重生",
    Desc = "FIN",
    Locked = false,
    Callback = function(state)
        local autoCollectRebirthRequestOrb = state
        
        if autoCollectRebirthRequestOrb then
                 if not LocalPlayer.Character or not LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
                Character = waitForCharacter()
                hum = Character:WaitForChild("HumanoidRootPart")
            end

            if rebirthRequestOrbConnection then
                rebirthRequestOrbConnection:Disconnect()
            end

            rebirthOrbConnection = game:GetService("RunService").Heartbeat:Connect(function()
                local args = { "rebirthRequest"}
                game:GetService("ReplicatedStorage").rEvents.rebirthEvent:FireServer(unpack(args))
            end)
        else
            if rebirthRequestirthOrbConnection then
                rebirthRequestirthOrbConnection:Disconnect()
                rebirthRequestirthOrbConnection = nil
            end
        end
    end
})

