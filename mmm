            local function survivoresp(char)
                local billboard = Instance.new("BillboardGui")
                billboard.Size = UDim2.new(2, 0, 0.5, 0) 
                billboard.StudsOffset = Vector3.new(0, 1.25, 0) 
                billboard.Adornee = char.Head
