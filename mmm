local StaminaSettings = {
    StaminaLoss = 10,
    StaminaGain = 25,
    InfiniteGain = 9999
}

local function findFirstChildRecursive(parent, name)
    if parent.Name == name then
        return parent
    end
    
    for _, child in ipairs(parent:GetChildren()) do
        local result = findFirstChildRecursive(child, name)
        if result then
            return result
        end
    end
    
    return nil
end

